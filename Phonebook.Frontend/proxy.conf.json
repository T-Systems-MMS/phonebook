{
  "/api/*": {
    "target": "http://localhost:8080",
    "secure": false,
    "changeOrigin": true,
    "pathRewrite": {
      "^/api": ""
    },
    "logLevel": "debug"
  },
  "/external_assets/*": {
    "target": "http://localhost:8081",
    "secure": false,
    "changeOrigin": true,
    "pathRewrite": {
      "^/external_assets": ""
    },
    "logLevel": "debug"
  },
  "/changelog.md": {
    "target": "https://raw.githubusercontent.com/T-Systems-MMS/phonebook/master/docs/README.md",
    "secure": false,
    "changeOrigin": true,
    "pathRewrite": { "/changelog.md": "" },
    "logLevel": "debug"
  }
}
